{
  "name": "config",
  "description": "CNS Broker Config",
  "environments": [
    "production",
    "staging"
  ],
  "services": [
    "messages",
    "profiles",
    "nodes",
    "server"
  ],
  "messages": {
    "module": "mqtt",
    "protocol": "mqtts",
    "host": "cns.padi.io",
    "user": "padi",
    "pass": "cnscp",
    "port": "1883",
    "root": "cns",
    "subscribe": {
      "topic": "#",
      "qos": 0,
      "rap": true
    },
    "publish": {
      "retain": true
    }
  },
  "profiles": {
    "host": "cp.padi.io",
    "path": "/profiles",
    "cache": true
  },
  "server": {
    "host": "0.0.0.0",
    "port": "8080",
    "public": "public",
    "compress": "gzip"
  }
}
